{
	"trailingComma": "es5",
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"printWidth": 120,
	"bracketSpacing": true,
	"requirePragma": false,
	"bracketSameLine": false,
	"useTabs": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"proseWrap": "always",
	"plugins": ["prettier-plugin-tailwindcss"]
}
